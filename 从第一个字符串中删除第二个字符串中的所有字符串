
public static String deleteCharacterFromString(String str1,String str2){
        for(int i=0;i<str2.length();i++){
            while(str1.indexOf(str2.charAt(i))!=-1){
               int index=str1.indexOf(str2.charAt(i));
                str1= str1.substring(0,index)+str1.substring(index+1,str1.length());
            }
        }
        return str1;
    }
