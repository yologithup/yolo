package prime;
public class Prime {
    public static boolean isPrime(int n){
        boolean ret=true;
        for(int i=2;i<n/2;i++){
            if(n%i==0){
              ret=false;    
            }
            break;
        }
        return ret;
    }
    
    public static void main(String[] args) {
        for(int n=100;n<=200;n++){
            if(isPrime(n))
                System.out.printf("%d ",n);
        }
    }
}
